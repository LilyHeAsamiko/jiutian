​

'\n# 寻找对应关系\nprint("寻找对应关系")\nresult3_adv = model.wv.most_similar([\'Advanced\', \'users\'], [\'model\'], topn=3)\nfor item in result3:\n    print(item[0], item[1])\nprint("n")\n'

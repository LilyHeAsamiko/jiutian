# 寻找对应关系

print("寻找对应关系")

print(' "boy" is to "son" as "girl" is to ...? ')

result3 = model.wv.most_similar(['girl', 'son'], ['boy'], topn=3)

for item in result3:

    print(item[0], item[1])

print("n")

​

more_examples = ["she her he", "small smaller tall", "going went teaching"]

for example in more_examples:

    a, b, x = example.split()

    predicted = model.wv.most_similar([x, b], [a])[0][0]

    print("'%s' is to '%s' as '%s' is to '%s'" % (a, b, x, predicted))

print("n================================")

